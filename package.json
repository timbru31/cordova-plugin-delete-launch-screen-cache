{
  "name": "cordova-plugin-delete-launch-screen-cache",
  "version": "1.0.2",
  "description": "Apache Cordova plugin to programmatically delete the iOS launch screen cache on iOS 13+",
  "author": "Tim Brust <github@timbrust.de>",
  "license": "MIT",
  "repository": "timbru31/cordova-plugin-delete-launch-screen-cache",
  "bugs": {
    "url": "https://github.com/timbru31/cordova-plugin-delete-launch-screen-cache/issues"
  },
  "scripts": {
    "lint": "npm run lint:ios && npm run lint:typescript",
    "lint:ios": "swiftlint",
    "lint:typescript": "eslint 'www/**/*.ts'",
    "build": "rm -rf dist && tsc",
    "prerelease": "npm run build",
    "release": "standard-version -s -a"
  },
  "devDependencies": {
    "@types/cordova": "^11.0.0",
    "@typescript-eslint/eslint-plugin": "^6.4.0",
    "@typescript-eslint/parser": "^6.4.0",
    "eslint": "^9.3.0",
    "eslint-plugin-prettier": "^5.0.0",
    "prettier": "^3.0.2",
    "replace": "^1.2.2",
    "standard-version": "^9.5.0",
    "typescript": "^5.1.6"
  },
  "cordova": {
    "id": "cordova-plugin-delete-launch-screen-cache",
    "platforms": [
      "ios"
    ]
  },
  "engines": {
    "cordovaDependencies": {
      "1.0.0": {
        "cordova": ">= 9.0.0",
        "cordova-ios": ">=5.0.0"
      }
    }
  },
  "keywords": [
    "cordova",
    "cordova-plugin",
    "ecosystem:cordova",
    "cordova-ios",
    "xcode",
    "launch-screen"
